{
  "functions": {
    "api/[[...slug]].js": {
      "maxDuration": 30
    }
  },
  "env": {
    "FIREBASE_PROJECT_ID": "shevoo-store",
    "REACT_APP_API_BASE": "https://barbari-store01-b3g8.vercel.app/api"
  },
  "build": {
    "env": {
      "REACT_APP_API_BASE": "https://barbari-store01-b3g8.vercel.app/api"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[[...slug]]"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://barbari-store01-b3g8.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}